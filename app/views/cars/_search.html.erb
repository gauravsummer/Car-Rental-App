<%= form_tag(cars_path, :method => "get", id: "search-form") do %>
    Search by
    <%= select_tag 'search_by', options_for_select(@options_for_search)  %>
    <%= text_field_tag :search, params[:search], placeholder: 'Search' %>
    <%= submit_tag 'Search', :name => nil, class: 'btn btn-primary btn-md text-white' %>
<% end %>